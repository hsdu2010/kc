<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sf.shiva.oms.ht.mapper.extend.WebserviceMgrExtendMapper">
  <resultMap id="BaseResultMap" type="com.sf.shiva.oms.ht.domain.WebserviceManager">
    <id column="id" jdbcType="VARCHAR" property="id" />
    <result column="url" jdbcType="VARCHAR" property="url" />
    <result column="method" jdbcType="VARCHAR" property="method" />
    <result column="createtime" jdbcType="TIMESTAMP" property="createtime" />
    <result column="updatetime" jdbcType="TIMESTAMP" property="updatetime" />
    <result column="name" jdbcType="VARCHAR" property="name" />
    <result column="create_emp" jdbcType="VARCHAR" property="createEmp" />
    <result column="modify_emp" jdbcType="VARCHAR" property="modifyEmp" />
  </resultMap>
  <resultMap extends="BaseResultMap" id="ResultMapWithBLOBs" type="com.sf.shiva.oms.ht.domain.WebserviceManager">
    <result column="requestbody" jdbcType="LONGVARCHAR" property="requestbody" />
  </resultMap>
  <sql id="Base_Column_List">
    id, url, method, createtime, updatetime, name, create_emp, modify_emp
  </sql>
  <sql id="Blob_Column_List">
    requestbody
  </sql>
  <select id="searchByName" resultMap="BaseResultMap">
    SELECT 
    <include refid="Base_Column_List" />
    ,
    <include refid="Blob_Column_List" />
    FROM tm_webservice_manager
    WHERE name LIKE #{name,jdbcType=VARCHAR}
    ORDER BY createtime desc
  </select>
</mapper>