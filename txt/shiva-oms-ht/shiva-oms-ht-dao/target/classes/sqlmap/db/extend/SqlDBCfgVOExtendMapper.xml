<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sf.shiva.oms.ht.mapper.extend.SqlDBCfgVOExtendMapper">
    <resultMap id="BaseResultMap" type="com.sf.shiva.oms.ht.domain.vo.SqlDBCfgVO">
        <id column="sql_id" jdbcType="VARCHAR" property="sqlId" />
        <result column="db_id" jdbcType="VARCHAR" property="dbId" />
        <result column="conn_name" jdbcType="VARCHAR" property="connName" />
        <result column="sql_statement" jdbcType="VARCHAR" property="sqlStatement" />
        <result column="sql_name" jdbcType="VARCHAR" property="sqlName" />
        <result column="description" jdbcType="VARCHAR" property="description" />
        <result column="create_tm" jdbcType="TIMESTAMP" property="createTm" />
        <result column="create_emp" jdbcType="VARCHAR" property="createEmp" />
        <result column="modify_tm" jdbcType="TIMESTAMP" property="modifyTm" />
        <result column="modify_emp" jdbcType="VARCHAR" property="modifyEmp" />
    </resultMap>
    <select id="selectSqlDBCfg" resultMap="BaseResultMap">
    SELECT sql_id, s.db_id db_id, conn_name, sql_statement, sql_name, description, s.create_tm create_tm,
    s.create_emp create_emp,s.modify_tm modify_tm, s.modify_emp modify_emp
    FROM tm_sql_cfg s, tm_db_conn_cfg d WHERE s.db_id = d.db_id
  </select>
  <select id="selectSqlDBCfgById" resultMap="BaseResultMap" parameterType="java.lang.String">
    SELECT sql_id, s.db_id db_id, conn_name, sql_statement, sql_name, description, s.create_tm create_tm,
    s.create_emp create_emp,s.modify_tm modify_tm, s.modify_emp modify_emp
    FROM tm_sql_cfg s, tm_db_conn_cfg d WHERE s.db_id = d.db_id AND sql_id = #{sqlId,jdbcType=VARCHAR}
  </select>
</mapper>